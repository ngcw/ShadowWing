import org.newdawn.slick.SlickException;

public class Item extends GameObject{

	/** Creates an Item
	 * 
	 * @param x				The initial X-Coordinates of an item.
	 * @param y				The initial Y-Coordinates of an item.
	 * @param image_path	The image path of an item.
	 * @throws SlickException
	 */
	public Item(double x, double y, String image_path) 
			throws SlickException {
		super(x, y, image_path);
	}

	/** This method is called when an item is picked up by the player. 
	 * 
	 * @param player The player in the game. 
	 */
	public boolean pickedup(Player player) {
		return false;
	}
}

